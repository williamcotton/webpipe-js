{
  "name": "webpipe-js",
  "version": "0.1.7",
  "description": "Web Pipe parser",
  "license": "ISC",
  "author": "William Cotton",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/parser.ts",
    "parse": "node dist/index.mjs",
    "test": "vitest run"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
